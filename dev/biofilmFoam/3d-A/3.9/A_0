FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "3.9";
    object      A_0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 0;

boundaryField
{
    z0
    {
        type            zeroGradient;
    }
    z1
    {
        type            fixedValue;
        value           uniform 0;
    }
    x0
    {
        type            zeroGradient;
    }
    x1
    {
        type            zeroGradient;
    }
    y0
    {
        type            zeroGradient;
    }
    y1
    {
        type            zeroGradient;
    }
}


