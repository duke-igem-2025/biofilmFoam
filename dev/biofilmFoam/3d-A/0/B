FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      B;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 0;

boundaryField
{
  x0
    {
      type zeroGradient;
    }
  
  x1
    {
      type zeroGradient;
    }

  y0
    {
      type empty;
    }
  
  y1
    {
      type empty;
    }
  
  z0
    {
      type zeroGradient;
    }

  z1
    {
      type zeroGradient;
    }
}

