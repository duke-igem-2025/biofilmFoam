(
    z0
    {
        type patch; // bottom (biofilm base)
        faces ((0 1 2 3));
    }

    z1
    {
        type patch; // top (nutrient inlet)
        faces ((4 5 6 7));
    }

    x0
    {
        type patch;
        faces ((0 4 7 3));
    }

    x1
    {
        type patch;
        faces ((1 5 6 2));
    }

    y0
    {
        type patch; // thin direction (out of plane)
        faces ((0 1 5 4));
