(
    (0          0         0)
    (1.2e-3     0         0)
    (1.2e-3     1.89e-6   0)
    (0          1.89e-6   0)
    (0          0         1.2e-3)
    (1.2e-3     0         1.2e-3)
    (1.2e-3     1.89e-6   1.2e-3)
    (0          1.89e-6   1.2e-3)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (120 1 120) simpleGrading (1 1 1)
);

