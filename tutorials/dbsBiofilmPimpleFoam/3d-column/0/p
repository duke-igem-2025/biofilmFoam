FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
  z1
    {
      type            zeroGradient;
    }

  z0
    {
      type            fixedValue;
      value           $internalField;
    }

  y1
    {
      type            zeroGradient;
    }
    
  y0
    {
      type            zeroGradient;
    }

  x1
    {
      type            zeroGradient;
    }
    
  x0
    {
      type            zeroGradient;
    }

  sphere1
    {
      type            zeroGradient;
    }
    
  sphere2
    {
      type            zeroGradient;
    }

  sphere3
    {
      type            zeroGradient;
    }
    
  cylinder
    {
      type            zeroGradient;
    }  
}


