FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      C;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
  top
    {
      type empty;
    }

  left
    {
      type zeroGradient;
    }

  right
    {
      type fixedValue;
      value uniform 1;
    }

  bottom
    {
      type empty;
    }

  defaultFaces
    {
      type empty;
    }
}

