FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues ( volScalarFieldValue M 0 );

regions
(

 boxToCell {
   box (4.96e-05 -100 -100) (5.2799999999999996e-05 100 100);
   fieldValues ( volScalarFieldValue M 0.8 );
 }

 );

