FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      C;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
  z1
    {
      type fixedValue;
      value uniform 1;
    }
  
  z0
    {
      type zeroGradient;
    }

  solid
    {
      type zeroGradient;
    }
}

